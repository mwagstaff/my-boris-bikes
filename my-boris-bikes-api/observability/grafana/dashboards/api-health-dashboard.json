{
  "title": "Request Overview",
  "schemaVersion": 38,
  "version": 9,
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status_code=~\"^[1-3][0-9][0-9]$\"}[$__rate_interval]))",
          "legendFormat": "good (HTTP < 400)"
        },
        {
          "expr": "sum(rate(http_requests_total{status_code=~\"^[4-5][0-9][0-9]$\"}[$__rate_interval]))",
          "legendFormat": "bad (HTTP >= 400)"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Bad Requests by Route (HTTP >= 400)",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status_code=~\"^[4-5][0-9][0-9]$\"}[$__rate_interval])) by (route)",
          "legendFormat": "{{route}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Good Requests by Route (HTTP < 400)",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status_code=~\"^[1-3][0-9][0-9]$\"}[$__rate_interval])) by (route)",
          "legendFormat": "{{route}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Request Latency",
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[$__rate_interval])) by (le))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Unique Users",
      "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "unique_users{period=\"1h\"}",
          "legendFormat": "1h unique users"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top Bad Requests by Route (HTTP >= 400)",
      "gridPos": { "x": 0, "y": 24, "w": 12, "h": 10 },
      "targets": [
        {
          "expr": "topk(10, max_over_time((sum(rate(http_requests_total{status_code=~\"^[4-5][0-9][0-9]$\"}[5m])) by (method, route, status_code))[$__range:]))",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "job": true,
              "instance": true,
              "Metric": true,
              "metric": true
            },
            "indexByName": {
              "method": 0,
              "route": 1,
              "status_code": 2,
              "Value": 3,
              "Value #A": 3
            },
            "renameByName": {
              "method": "Method",
              "route": "Route",
              "status_code": "Status code",
              "Value": "Count",
              "Value #A": "Count"
            }
          }
        }
      ]
    },
    {
      "type": "table",
      "title": "Top Good Requests by Route (HTTP < 400)",
      "gridPos": { "x": 12, "y": 24, "w": 12, "h": 10 },
      "targets": [
        {
          "expr": "topk(10, max_over_time((sum(rate(http_requests_total{status_code=~\"^[1-3][0-9][0-9]$\"}[5m])) by (method, route, status_code))[$__range:]))",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "job": true,
              "instance": true,
              "Metric": true,
              "metric": true
            },
            "indexByName": {
              "method": 0,
              "route": 1,
              "status_code": 2,
              "Value": 3,
              "Value #A": 3
            },
            "renameByName": {
              "method": "Method",
              "route": "Route",
              "status_code": "Status code",
              "Value": "Count",
              "Value #A": "Count"
            }
          }
        }
      ]
    },
    {
      "type": "table",
      "title": "Top Routes by Latency",
      "gridPos": { "x": 0, "y": 34, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "topk(10, max_over_time((histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[$__rate_interval])) by (le, route)))[$__range:]))",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "job": true,
              "instance": true,
              "Metric": true,
              "metric": true,
              "le": true
            },
            "indexByName": {
              "route": 0,
              "Value": 1,
              "Value #A": 1
            },
            "renameByName": {
              "route": "Route",
              "Value": "P95 latency",
              "Value #A": "P95 latency"
            }
          }
        }
      ]
    }
  ]
}
