{
  "title": "TfL API Calls",
  "schemaVersion": 38,
  "version": 2,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Good Requests (1xx-3xx) - req/sec",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(tfl_requests_total{status_code=~\"^[1-3][0-9][0-9]$\"}[$__rate_interval]))",
          "legendFormat": "good req/sec"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Good Requests by Status Code - req/sec",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(tfl_requests_total{status_code=~\"^[1-3][0-9][0-9]$\"}[$__rate_interval])) by (status_code)",
          "legendFormat": "{{status_code}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Bad Requests (4xx-5xx + error) - req/sec",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "fixed",
            "fixedColor": "#E02F44"
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(tfl_requests_total{status_code=~\"(^[4-5][0-9][0-9]$|^error$|^unknown$)\"}[$__rate_interval]))",
          "legendFormat": "bad req/sec"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Bad Requests by Status Code - req/sec",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "fixed",
            "fixedColor": "#E02F44"
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(tfl_requests_total{status_code=~\"(^[4-5][0-9][0-9]$|^error$|^unknown$)\"}[$__rate_interval])) by (status_code)",
          "legendFormat": "{{status_code}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Dock Poll Rate",
      "gridPos": { "x": 0, "y": 16, "w": 24, "h": 8 },
      "targets": [
        { "expr": "rate(dock_polls_total[5m])" }
      ]
    },
    {
      "type": "table",
      "title": "Top URLs by Count and Status",
      "gridPos": { "x": 0, "y": 24, "w": 24, "h": 10 },
      "targets": [
        {
          "expr": "sort_desc(topk(20, sum(increase(tfl_requests_total[$__range])) by (url_path, status_code)))",
          "instant": true,
          "format": "table"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "job": true,
              "instance": true,
              "Metric": true,
              "metric": true
            },
            "indexByName": {
              "url_path": 0,
              "status_code": 1,
              "Value": 2,
              "Value #A": 2
            },
            "renameByName": {
              "url_path": "URL",
              "status_code": "Status code",
              "Value": "Count",
              "Value #A": "Count"
            }
          }
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "P95 Latency (All TfL Requests)",
      "gridPos": { "x": 0, "y": 34, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "1000 * histogram_quantile(0.95, sum(rate(tfl_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Error Rate (% of TfL Requests)",
      "gridPos": { "x": 12, "y": 34, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "100 * sum(rate(tfl_requests_total{status_code=~\"(^[4-5][0-9][0-9]$|^error$|^unknown$)\"}[$__rate_interval])) / sum(rate(tfl_requests_total[$__rate_interval]))",
          "legendFormat": "error %"
        }
      ]
    }
  ]
}
